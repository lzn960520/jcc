using System::IO;

module sort begin

/***
 * the class is the Main class
 */
class Main
begin
	private int[0..10] a; // the array

	private procedure sort(int l, int r)
	begin
		var int i = l;
		var int j = r;
		var int mid = a[(l + r) / 2];
		repeat
			while a[i] < mid do i++;
			while a[j] > mid do j--;
			if i <= j then
			begin
				var int temp = a[i];
				a[i] = a[j];
				a[j] = temp;
				i++;
				j--;
			end
		until i > j;
		if i < r then
			sort(i, r);
		if l < j then
			sort(l, j);
	end;
	
	static public function main() : int
	begin
		var Main that = new Main;
		that.a[0] = 1;
		that.a[1] = 5;
		that.a[2] = 4;
		that.a[3] = 7;
		that.a[4] = 10;
		that.a[5] = 8;
		that.sort(0, 5);
		System::IO::writeInt(that.a[0]);
		System::IO::writeInt(that.a[1]);
		System::IO::writeInt(that.a[2]);
		System::IO::writeInt(that.a[3]);
		System::IO::writeInt(that.a[4]);
		System::IO::writeInt(that.a[5]);
		return 0;
	end;
end;

end;
